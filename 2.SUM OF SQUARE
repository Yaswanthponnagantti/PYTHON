def sumsquare(l):
    odd=[ ]
    even=[ ]
    l3=[ ]
    for i in range(0,len(l)):
        if(l[i]%2==0):
            even.append(l[i])
        else:
            odd.append(l[i])
    print(odd,even)
    oddsquare=sum([x**2 for x in odd])
    l3.append(oddsquare)
    evensquare=sum([y**2 for y in even])
    l3.append(evensquare)
    return l3

n=int(input("n="))
l1=[ ]
while True:
    num=int(input("enter the element"))
    if num==" ":
        breake
    else:
        l1.append(num)
        if len(l1)==n:
            break
print(l1)
print(sumsquare(l1))
